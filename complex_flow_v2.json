{
  "description": "Complex Test Call Tree",
  "states": [
    {
      "name": "Trigger",
      "type": "trigger",
      "transitions": [
        {"event": "incomingMessage"},
        {"event": "incomingCall", "next": "main_menu"},
        {"event": "incomingConversationMessage"},
        {"event": "incomingRequest"}
      ],
      "properties": {
        "offset": {"x": 0, "y": 0}
      }
    },
    {
      "name": "main_menu",
      "type": "gather-input-on-call",
      "transitions": [
        {"event": "keypress", "next": "main_router"},
        {"event": "speech", "next": "main_router"},
        {"event": "timeout", "next": "main_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": 0, "y": 200},
        "say": "Thank you for calling Sunset Ridge Apartments. Press 1 for leasing. Press 2 for current residents. Press 3 for maintenance. Press 0 for operator.",
        "timeout": 8
      }
    },
    {
      "name": "main_router",
      "type": "split-based-on",
      "transitions": [
        {"event": "noMatch", "next": "main_menu"},
        {"event": "match", "next": "leasing_menu", "conditions": [{"friendly_name": "If value equal_to 1", "arguments": ["{{widgets.main_menu.Digits}}"], "type": "equal_to", "value": "1"}]},
        {"event": "match", "next": "residents_menu", "conditions": [{"friendly_name": "If value equal_to 2", "arguments": ["{{widgets.main_menu.Digits}}"], "type": "equal_to", "value": "2"}]},
        {"event": "match", "next": "maintenance_vm", "conditions": [{"friendly_name": "If value equal_to 3", "arguments": ["{{widgets.main_menu.Digits}}"], "type": "equal_to", "value": "3"}]},
        {"event": "match", "next": "operator_hold", "conditions": [{"friendly_name": "If value equal_to 0", "arguments": ["{{widgets.main_menu.Digits}}"], "type": "equal_to", "value": "0"}]}
      ],
      "properties": {
        "input": "{{widgets.main_menu.Digits}}",
        "offset": {"x": 0, "y": 400}
      }
    },
    {
      "name": "leasing_menu",
      "type": "gather-input-on-call",
      "transitions": [
        {"event": "keypress", "next": "leasing_router"},
        {"event": "speech", "next": "leasing_router"},
        {"event": "timeout", "next": "leasing_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -300, "y": 600},
        "say": "Leasing department. Press 1 for rates. Press 2 for application status. Press 3 for tours. Press 4 to speak with an agent.",
        "timeout": 8
      }
    },
    {
      "name": "leasing_router",
      "type": "split-based-on",
      "transitions": [
        {"event": "noMatch", "next": "leasing_menu"},
        {"event": "match", "next": "rates_info", "conditions": [{"friendly_name": "If value equal_to 1", "arguments": ["{{widgets.leasing_menu.Digits}}"], "type": "equal_to", "value": "1"}]},
        {"event": "match", "next": "app_menu", "conditions": [{"friendly_name": "If value equal_to 2", "arguments": ["{{widgets.leasing_menu.Digits}}"], "type": "equal_to", "value": "2"}]},
        {"event": "match", "next": "tour_menu", "conditions": [{"friendly_name": "If value equal_to 3", "arguments": ["{{widgets.leasing_menu.Digits}}"], "type": "equal_to", "value": "3"}]},
        {"event": "match", "next": "human_transfer", "conditions": [{"friendly_name": "If value equal_to 4", "arguments": ["{{widgets.leasing_menu.Digits}}"], "type": "equal_to", "value": "4"}]}
      ],
      "properties": {
        "input": "{{widgets.leasing_menu.Digits}}",
        "offset": {"x": -300, "y": 800}
      }
    },
    {
      "name": "residents_menu",
      "type": "gather-input-on-call",
      "transitions": [
        {"event": "keypress", "next": "residents_router"},
        {"event": "timeout", "next": "residents_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": 0, "y": 600},
        "say": "Current residents. Press 1 for rent payment. Press 2 for lease renewal. Press 3 for maintenance.",
        "timeout": 8
      }
    },
    {
      "name": "residents_router",
      "type": "split-based-on",
      "transitions": [
        {"event": "noMatch", "next": "residents_menu"},
        {"event": "match", "next": "ai_assistant", "conditions": [{"friendly_name": "If value equal_to 2", "arguments": ["{{widgets.residents_menu.Digits}}"], "type": "equal_to", "value": "2"}]}
      ],
      "properties": {
        "input": "{{widgets.residents_menu.Digits}}",
        "offset": {"x": 0, "y": 800}
      }
    },
    {
      "name": "app_menu",
      "type": "gather-input-on-call",
      "transitions": [
        {"event": "keypress", "next": "app_router"},
        {"event": "timeout", "next": "app_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -500, "y": 1000},
        "say": "Application status. Press 1 for online check. Press 2 to speak with processing team.",
        "timeout": 8
      }
    },
    {
      "name": "app_router",
      "type": "split-based-on",
      "transitions": [
        {"event": "noMatch", "next": "app_menu"},
        {"event": "match", "next": "online_info", "conditions": [{"friendly_name": "If value equal_to 1", "arguments": ["{{widgets.app_menu.Digits}}"], "type": "equal_to", "value": "1"}]},
        {"event": "match", "next": "human_transfer", "conditions": [{"friendly_name": "If value equal_to 2", "arguments": ["{{widgets.app_menu.Digits}}"], "type": "equal_to", "value": "2"}]}
      ],
      "properties": {
        "input": "{{widgets.app_menu.Digits}}",
        "offset": {"x": -500, "y": 1200}
      }
    },
    {
      "name": "tour_menu",
      "type": "gather-input-on-call",
      "transitions": [
        {"event": "keypress", "next": "tour_router"},
        {"event": "timeout", "next": "tour_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -200, "y": 1000},
        "say": "Tour scheduling. Press 1 for self-guided tour. Press 2 to schedule with an agent.",
        "timeout": 8
      }
    },
    {
      "name": "tour_router",
      "type": "split-based-on",
      "transitions": [
        {"event": "noMatch", "next": "tour_menu"},
        {"event": "match", "next": "selfguided_info", "conditions": [{"friendly_name": "If value equal_to 1", "arguments": ["{{widgets.tour_menu.Digits}}"], "type": "equal_to", "value": "1"}]},
        {"event": "match", "next": "human_transfer", "conditions": [{"friendly_name": "If value equal_to 2", "arguments": ["{{widgets.tour_menu.Digits}}"], "type": "equal_to", "value": "2"}]}
      ],
      "properties": {
        "input": "{{widgets.tour_menu.Digits}}",
        "offset": {"x": -200, "y": 1200}
      }
    },
    {
      "name": "rates_info",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete", "next": "leasing_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -600, "y": 1000},
        "say": "Studios from $1,200. One beds $1,450. Two beds from $1,850. Three beds from $2,400."
      }
    },
    {
      "name": "online_info",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete", "next": "app_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -700, "y": 1200},
        "say": "Check your application status online at our website under My Application."
      }
    },
    {
      "name": "selfguided_info",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete", "next": "tour_menu"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": -100, "y": 1400},
        "say": "Self-guided tours available 7 days a week, 9 AM to 6 PM. Check in at the leasing office."
      }
    },
    {
      "name": "maintenance_vm",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": 300, "y": 600},
        "say": "Maintenance department. Leave your name, unit, and issue after the tone. Beep!"
      }
    },
    {
      "name": "operator_hold",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete", "next": "human_transfer"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": 300, "y": 400},
        "say": "Please hold for the next available representative."
      }
    },
    {
      "name": "ai_assistant",
      "type": "say-play",
      "transitions": [
        {"event": "audioComplete", "next": "human_transfer"}
      ],
      "properties": {
        "voice": "Polly.Joanna",
        "offset": {"x": 100, "y": 1000},
        "say": "Hi! I'm Mia, your Virtual Leasing Assistant powered by EliseAI. I can help with pricing and availability. I didn't catch that. Let me connect you."
      }
    },
    {
      "name": "human_transfer",
      "type": "connect-call-to",
      "transitions": [
        {"event": "callCompleted"}
      ],
      "properties": {
        "offset": {"x": -300, "y": 1600},
        "to": "+12022158237",
        "timeout": 30
      }
    }
  ],
  "initial_state": "Trigger",
  "flags": {
    "allow_concurrent_calls": true
  }
}









